{
  "name": "lets-form",
  "version": "0.1.10",
  "description": "JSON to React form generator, supporting multiple frameworks (Bootstrap, Material UI, RSuite, etc)",
  "main": "dist/lets-form.min.js",
  "module": "dist/lets-form.min.js",
  "scripts": {
    "prebuild": "./scripts/copy-all-files.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-rsuite5 && npm run build-antd && echo \"\n\nAll done.\"",
    "build:webpack": "webpack --mode production --progress",
    "build-rsuite5": "npm run build:webpack -- --env framework=\"rsuite5\"",
    "build-antd": "npm run build:webpack -- --env framework=\"antd\"",
    "build-react": "npm run build:webpack -- --env framework=\"react\""
  },
  "author": "Guido Bellomo",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "react-bootstrap": "^2.7.2",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-proto-to-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime-corejs2": "^7.8.4",
    "@mui/x-date-pickers": "^6.0.3",
    "antd": "^5.3.3",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-dev": "^2.0.1",
    "cross-env": "^6.0.3",
    "css-loader": "^6.7.3",
    "micro-down": "^1.6.2",
    "react-hook-form": "^7.43.7",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.2",
    "webpack": "^5.76.2",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^5.0.1"
  }
}
